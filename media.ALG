Algoritmo "me d i a

Var
N1, N2 ,M : REAL

Inicio
 	Escreva ( "Primeira nota: " )
		Leia ( N1 )
	Escreva ( "Segunda Nota:")
		Leia (N2)
	M <- (N1 + N2) / 2
	Escreval( " A media do aluno foi " , M :4:2)
	Se ( M >= 7) então
		Escreval("Aluno APROVADO)
	Senão
		Se ( M>=5) e ( M <7) então
      Escreval("Aluno em RECUPERAÇÂO)
	Senão
		Escreval("Aluno REPROVADO " )
	FimSe
FimSe



Fimalgoritmo